        <StackPanel Margin="20" Background="Green">
            <TextBlock Text="Booking History" FontSize="24" Foreground="White" HorizontalAlignment="Center"/>
            <ListBox x:Name="HistoryList" Margin="10" Height="300"/>
        </StackPanel>
    </Grid>
</Page>